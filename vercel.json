{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/actions/demo-upload/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/actions/demo-upload-optimized/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/actions/upload/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/actions/upload-optimized/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/analyze-scorecard/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/transcribe-voice/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/generate-summary/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "env": {
    "VERCEL_PRO": "true"
  },
  "crons": [
    {
      "path": "/api/notifications/generate",
      "schedule": "0 8 * * *"
    }
  ]
} 